{
	"repository": "pyod.models.xgbod.XGBOD",
	"components": [
		{
			"name": "pyod.models.xgbod.XGBOD",
			"providedInterface": [
				"AbstractDetector",
				"BasicDetector",
				"BaseLearner"
			],
			"requiredInterface": [
				{
					"id": "estimator_list",
					"name": "BasicDetector",
					"min": 2,
					"max": 5
				}
			],
			"parameters": [
				{
                    "name": "max_depth",
                    "type": "int",
                    "default": 3,
                    "min": 1,
                    "max": 30,
                    "minInterval": 1,
                    "refineSplits": 2
                },
                {
					"name": "learning_rate",
					"type": "double",
					"default": 0.001,
					"min": 0.0,
					"max": 1.0,
					"minInterval": 0.001,
					"refineSplits": 2
				},
				{
                    "name": "n_estimators",
                    "type": "int",
                    "default": 100,
                    "min": 1,
                    "max": 300,
                    "minInterval": 10,
                    "refineSplits": 2
                },
                {
                    "name": "objective",
                    "type": "cat",
                    "values": [
                         "binary:logistic",
                         "binary:logitraw",
                         "binary:hinge"
                    ],
                    "default": "binary:logistic"
                },
                {
                    "name": "booster",
                    "type": "cat",
                    "values": [
                         "gbtree",
                         "gblinear",
                         "dart"
                    ],
                    "default": "gbtree"
                },
                {
                    "name": "n_jobs",
                    "type": "int",
                    "default": -1,
                    "min": -1,
                    "max": -1,
                    "minInterval": 1,
                    "refineSplits": 2
                },
                {
                    "name": "gamma",
                    "type": "double",
                    "default": 0.0,
                    "min": 0.00,
                    "max": 1,
                    "minInterval": 0.001,
                    "refineSplits": 2
                },
                {
                    "name": "min_child_weight",
                    "type": "int",
                    "default": 1,
                    "min": 0,
                    "max": 10,
                    "minInterval": 1,
                    "refineSplits": 2
                },
                {
                    "name": "max_delta_step",
                    "type": "int",
                    "default": 0,
                    "min": 0,
                    "max": 10,
                    "minInterval": 1,
                    "refineSplits": 2
                },
                {
                    "name": "subsample",
                    "type": "double",
                    "default": 1.0,
                    "min": 0.00,
                    "max": 1.0,
                    "minInterval": 0.001,
                    "refineSplits": 2
                },
                {
                    "name": "colsample_bytree",
                    "type": "double",
                    "default": 1.0,
                    "min": 0.00,
                    "max": 1.0,
                    "minInterval": 0.001,
                    "refineSplits": 2
                },
                {
                    "name": "colsample_bylevel",
                    "type": "double",
                    "default": 1.0,
                    "min": 0.00,
                    "max": 1.0,
                    "minInterval": 0.001,
                    "refineSplits": 2
                },
                {
					"name": "reg_alpha",
                    "type": "double",
                    "default": 0.0,
                    "min": 0.0,
                    "max": 1.0,
                    "minInterval": 0.01,
                    "refineSplits": 2
				},
				{
					"name": "reg_lambda",
                    "type": "double",
                    "default": 1.0,
                    "min": 0.0,
                    "max": 1.0,
                    "minInterval": 0.01,
                    "refineSplits": 2
				},
				{
					"name": "scale_pos_weight",
                    "type": "double",
                    "default": 1.0,
                    "min": 0.0,
                    "max": 1.0,
                    "minInterval": 0.01,
                    "refineSplits": 2
				},
				{
					"name": "base_score",
                    "type": "double",
                    "default": 0.5,
                    "min": 0.0,
                    "max": 1.0,
                    "minInterval": 0.01,
                    "refineSplits": 2
				}
			]
		}
	]
}