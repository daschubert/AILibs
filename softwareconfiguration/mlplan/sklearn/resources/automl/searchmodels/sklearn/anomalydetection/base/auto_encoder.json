{
	"repository": "pyod_wrapper.auto_encoder.AutoEncoderWrapper",
	"components": [
		{
			"name": "pyod_wrapper.auto_encoder.AutoEncoderWrapper",
			"providedInterface": [
				"AbstractDetector",
				"BasicDetector",
				"BaseLearner"
			],
			"requiredInterface": [],
			"parameters": [
				{
					"name": "contamination",
					"type": "double",
					"default": 0.1,
					"min": 0.0,
					"max": 0.5,
					"minInterval": 0.01,
					"refineSplits": 2
				},
				{
					"name": "min_nbr_neurons",
					"type": "int",
					"default": "{$DEF_MIN_NBR_NEURONS}",
					"min": 1,
					"max": "{$MAX_MIN_NBR_NEURONS}",
					"minInterval": 1,
					"refineSplits": 8
				},
				{
					"name": "hidden_activation",
					"type": "cat",
					"values": [
						"relu",
						"sigmoid",
						"softmax",
						"softplus",
						"softsign",
						"tanh",
						"selu",
						"elu",
						"exponential"
					],
					"default": "relu"
				},
				{
					"name": "output_activation",
					"type": "cat",
					"values": [
						"relu",
						"sigmoid",
						"softmax",
						"softplus",
						"softsign",
						"tanh",
						"selu",
						"elu",
						"exponential"
					],
					"default": "sigmoid"
				},
				{
					"name": "loss",
					"type": "cat",
					"values": [
						"binary_crossentropy",
						"poisson",
						"binary_crossentropy",
						"kl_divergence",
						"mean_squared_error",
						"mean_absolute_error",
						"mean_absolute_percentage_error",
						"mean_squared_logarithmic_error",
						"cosine_similarity",
						"huber_loss",
						"log_cosh",
						"hinge",
						"squared_hinge"
					],
					"default": "mean_squared_error"
				},
				{
					"name": "optimizer",
					"type": "cat",
					"values": [
						"SGD",
						"RMSprop",
						"Adam",
						"Adadelta",
						"Adagrad",
						"Adamax",
						"Nadam",
						"Ftrl"
					],
					"default": "Adam"
				},
				{
					"name": "epochs",
					"type": "int",
					"default": 100,
					"min": 10,
					"max": 150,
					"minInterval": 1,
					"refineSplits": 2
				},
				{
					"name": "batch_size",
					"type": "int",
					"default": 32,
					"min": 2,
					"max": 100,
					"minInterval": 1,
					"refineSplits": 2
				},
				{
					"name": "dropout_rate",
					"type": "double",
					"default": 0.2,
					"min": 0.0,
					"max": 1.0,
					"minInterval": 0.01,
					"refineSplits": 2
				},
				{
					"name": "l2_regularizer",
                    "type": "double",
                    "default": 0.1,
                    "min": 0.0,
                    "max": 1.0,
                    "minInterval": 0.01,
                    "refineSplits": 2
				},
				{
					"name": "validation_size",
                    "type": "double",
                    "default": 0.0,
                    "min": 0.0,
                    "max": 0.0,
                    "minInterval": 0.01,
                    "refineSplits": 2
				},
				{
					"name": "preprocessing",
					"type": "cat",
					"values": [
						"True",
						"False"
					],
					"default": "True"
				},
				{
					"name": "verbose",
                    "type": "int",
                    "default":0,
                    "min": 0,
                    "max": 0,
                    "minInterval": 1,
                    "refineSplits": 2
				}
			]
		}
	]
}

